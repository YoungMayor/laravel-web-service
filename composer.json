{
    "name": "youngmayor/laravel-web-service",
    "description": "A laravel package for connecting to external services",
    "keywords": [
        "youngmayor",
        "laravel-web-service"
    ],
    "homepage": "https://github.com/youngmayor/laravel-web-service",
    "license": "MIT",
    "type": "library",
    "authors": [{
        "name": "Meyoron Aghogho",
        "email": "youngmayor.dev@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "YoungMayor\\LaravelWebService\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YoungMayor\\LaravelWebService\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "YoungMayor\\LaravelWebService\\LaravelWebServiceServiceProvider"
            ],
            "aliases": {
                "LaravelWebService": "YoungMayor\\LaravelWebService\\Facade\\LaravelWebService"
            }
        }
    }
}